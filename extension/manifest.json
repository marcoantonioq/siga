{
  "manifest_version": 3,
  "name": "Minha Extensão",
  "version": "1.0",
  "description": "Extensão para interagir com o SIGA",
  "permissions": [
    "identity",
    "activeTab",
    "scripting",
    "storage",
    "cookies"
  ],
  "host_permissions": [
    "https://siga.congregacao.org.br/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
